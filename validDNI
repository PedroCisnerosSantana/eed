var letras;
var numero;
var letr;
var dni;

letras = "TRWAGMYFPDXBNJZSQVHLCKET";

dni = prompt("Introduce your DNI: ");

numero = dni.substr(0, dni.length - 1);
// console.log(numero); // remove comments to see value

letr = dni.substr(dni.length - 1, 1);
// console.log(letr); // remove comments to see value

numero = numero % 23
// console.log(numero); // remove comments to see value

letra = letra.substring(numero, numero + 1);
// console.log(letra); // remove comments to see value

if (letra != letr.toUpperCase()) {
    alert('Wrong DNI. DNI letter is not correct')
} else {
    alert('Correct DNI');
} 